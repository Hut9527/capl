/*鍒嗛〉*/
#pageGro{ width: 300px; height:27px; margin:0 auto;cursor: pointer;}
/*#pageGro>div{padding-right: 5px;}*/
#pageGro input{ width: 40px;height: 27px;}
#pageGro div,#pageGro div ul li{ font-size:12px; color:#666; line-height:27px; float:left;}
#pageGro div ul li{ width:27px; background: #fafafa; text-align:center; border:1px solid #DDDDDD; cursor:pointer;border-right-width: 0;
    -webkit-box-shadow: inset 0px -2px 0px 0px rgba(0, 0, 0, 0.09);
    -moz-box-shadow: inset 0px -2px 0px 0px rgba(0, 0, 0, 0.09);
    box-shadow: inset 0px -1px 0px 0px rgba(0, 0, 0, 0.09);
}
#pageGro div ul li.on{ color:#666; background:#EEEEEE; border:1px solid #DDDDDD;border-right-width: 0;
    -webkit-box-shadow: inset 0px -1px 0px 0px rgba(0, 0, 0, 0.09);
    -moz-box-shadow: inset 0px -1px 0px 0px rgba(0, 0, 0, 0.09);
    box-shadow: inset 0px -1px 0px 0px rgba(0, 0, 0, 0.09);
}
#pageGro .pageUp,#pageGro .pageDown{ background: #fafafa; width:27px; border:1px solid #DDDDDD; text-indent:5px; cursor:pointer;
    -webkit-box-shadow: inset 0px -1px 0px 0px rgba(0, 0, 0, 0.09);
    -moz-box-shadow: inset 0px -1px 0px 0px rgba(0, 0, 0, 0.09);
    box-shadow: inset 0px -1px 0px 0px rgba(0, 0, 0, 0.09);}
#pageGro .pageUp{border-right-width: 0;}
#pageGro div ul li:hover, #pageGro .pageUp:hover,#pageGro .pageDown:hover{background: #EEEEEE;}


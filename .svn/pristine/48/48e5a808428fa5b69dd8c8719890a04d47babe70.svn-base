<script type="text/javascript">

//页面加载完成后执行
$(function(){
	for(var i = 2008 ; i < 2019 ; i ++){
		$("#year").append("<option value='"+i+"'>"+i+"</option>");
	}
});

//搜索
$("#search").click(function(){
	
	var year = $("#year").val();
	var qoq = $("#qoq").val();
	
	if(year != 0 && qoq != 0){
		var obj = document.getElementById('main');
		obj.style.visibility= "visible";
		$("#fm").html(year + "年第" + qoq + "季度");
		
		//首先清除之前的数据
		$("#tab1 tbody").empty();
		$("#tab2 tbody").empty();
		$("#tab4 tbody").empty();
		$("#tab6 tbody").empty();
		$("#tab7 tbody").empty();
		
		$.ajax({
			type : 'post',
			url : '${path}/quarter/selectByTime.do',
			async : true,
			dataType : 'json',
			data : {"year":year,"qoq":qoq},
			
			success : function(data){
				
				//tab1
				$.each(data.cartable1,function (n,v){
					var tab1 = $("#tab1");
					++n;
					//编号
					var bianhao = $("<td>"+n+"</td>")
					//车牌号
					var vehicle = $("<td>"+v.vehicle+"</td>");
					//日期
					var curvetime = $("<td>"+v.vehcurvetime+"</td>");
					//考勤情况
					var attendance = $("<td>"+v.vehattendance+"</td>");
					//工作时间
					var worktime = $("<td>"+v.vehworktime+"</td>");
					//工作里程
					var workkm = $("<td>"+v.vehworkkm+"</td>");
					//备注
					var remark = $("<td>"+v.vehremark+"</td>");
					
					var $tr = $("<tr></tr>")
					$tr.append(bianhao);
					$tr.append(vehicle);
					$tr.append(curvetime);
					$tr.append(attendance);
					$tr.append(worktime);
					$tr.append(workkm);
					$tr.append(remark);
					
					tab1.append($tr);
				});
				
				
				//tab2
				$.each(data.usertable2,function(n,v){
					var tab2 = $("#tab2");
					++n;
					//编号
					var bianhao = $("<td>"+n+"</td>");
					//工号
					var jobnumber = $("<td>"+v.peojobnumber+"</td>");
					//姓名
					var name = $("<td>"+v.peoname+"</td>");
					//单位
					var company = $("<td>"+v.peocompany+"</td>");
					//日期
					var curvetime = $("<td>"+v.peocurvetime+"</td>");
					//考勤情况
					var attendance = $("<td>"+v.peoattendance+"</td>");
					//工作时间
					var worktime = $("<td>"+v.peoworktime+"</td>");
					
					var $tr = $("<tr></tr>");
					
					$tr.append(bianhao);
					$tr.append(jobnumber);
					$tr.append(name);
					$tr.append(company);
					$tr.append(curvetime);
					$tr.append(attendance);
					$tr.append(worktime);
					
					tab2.append($tr);
				});
				
				
				//tab4
				$.each(data.usertable4,function(n,v){
					var tab4 = $("#tab4");
					++n;
					//编号
					var bianhao = $("<td>"+n+"</td>");
					//姓名
					var name = $("<td>"+v.peoviolatname+"</td>");
					//单位
					var company = $("<td>"+v.peoviolatcompany+"</td>");
					//类型
					var dat = $("<td>"+v.peoviolatdata+"</td>");
					
					var $tr = $("<tr></tr>");
					
					$tr.append(bianhao);
					$tr.append(name);
					$tr.append(company);
					$tr.append(dat);
					
					tab4.append($tr);
				});
				
				
				//tab6
				$.each(data.cartable6,function(n,v){
					var tab6 = $("#tab6");
					++n;
					//编号
					var bianhao = $("<td>"+n+"</td>");
					//车牌号
					var vehicle = $("<td>"+v.vehviolatvehicle+"</td>");
					//单位
					var company = $("<td>"+v.vehviolatcompany+"</td>");
					//类型
					var dat = $("<td>"+v.vehviolatdata+"</td>");
					
					var $tr = $("<tr></tr>");
					
					$tr.append(bianhao);
					$tr.append(vehicle);
					$tr.append(company);
					$tr.append(dat);
					
					tab6.append($tr);
				});
				
				
				//tab7
				$.each(data.cartable7,function(n,v){
					var tab7 = $("#tab7");

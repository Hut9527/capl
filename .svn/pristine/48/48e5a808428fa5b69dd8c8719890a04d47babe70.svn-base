//导出
$("#export").click(function(){
	//放入图片实际地址
	$.ajax({
		type : 'post',
		url : '${path}/img/getFactUrl.do',
		async : false,
		dataType : 'json',
		data : {
			
		},
		
		success : function (data) {
			var img = "";
			$.each(data,function(n,v){
				if(n < (data.length -1)){
					img += v += "/";
				} else {
					img += v;
				}
			});
			
			//放入src
			$("#img").attr('src',img);
			//导出
			$("#main").wordExport("word");
			console.log('${img}')
		}
	});
	
});

